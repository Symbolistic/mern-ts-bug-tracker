{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"outDir": "server/dist/",
		"rootDir": "server",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["server/src/**/*"],
	"exclude": ["server/src/types/*.ts", "node_modules", ".vscode"]
}
